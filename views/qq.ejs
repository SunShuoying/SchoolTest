                <dl class="my-msg">  <dt>会计1号&nbsp;13:04:01</dt>  <dd>哈哈</dd> </dl>
            </div>

			<div class="edit-tools">
				<a href="javascript:void(0);" class="tool-face" id="tool-face"></a>
			</div>
			<div class="chat-faces" id="chat-faces">
			<script type="text/javascript" id="loadFaces">
			(function() {
				var s = '';
				for(var i=0; i<=104; i++) {
					s += '<a href="###" class="face-icon" faceCode="[face' + i + ']"></a>';
				}
				document.write(s);
			}());
			</script>
			</div>
			<div class="edit-box" id="edit-box" contenteditable></div>

			<div class="chat-btns">
				<em>按Ctrl+Enter键发送消息</em>
				<a class="close-box" href="javascript:void(0);" id="close-box" hidefocus="true">关闭</a>
				<a class="send-msg" href="javascript:void(0);" id="send-msg" hidefocus="true">发送</a>
			</div>
		</div>
		<div class="info-card">
			<iframe id="info-frame" src="" width="196" height="422" frameborder="0" scrolling="no"></iframe>
		</div>
	</div>
</body>







