        <div class="form-group">
            <label>密码：</label>
            <input type="password" class="form-control" name="password" id="password" placeholder="请输入密码"/>
        </div>
        <input type="submit" class="btn btn-success" value="登录"/>
    </div>

</form>
<%- include footer %>