<%- include header %>
<div style="width:100%;position: relative;top: 0.5em;">
    <table class="table table-hover table-bordered table-striped table-condensed">
        <caption>签到详情</caption>
        <thead>
        <tr>
            <th>编号</th>
            <th>姓名</th>
            <th>性别</th>
            <th>第一学历</th>
            <th>专业</th>
            <th>入学年份</th>
            <th>学号</th>
            <th>手机</th>
            <th>邮箱</th>

        </tr>
        </thead>
        <tbody>
        <% signs.forEach(function (enter,index) {%>
        <tr>
            <td><%= index+1 %></td>
            <td><%= enter.name %></td>
            <td><%= enter.sex %></td>
            <td><%= enter.experience.experience %></td>
            <td><%= enter.experience.major %></td>
            <td><%= enter.experience.date %></td>
            <td><%= enter.experience.studentId %></td>
            <td><%= enter.telephone %></td>
            <td><%= enter.email %></td>
        </tr>
        <%})%>
        </tbody>
    </table>
</div>
<%- include footer %>